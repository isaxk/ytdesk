{
  "name": "ytdesk",
  "version": "1.0.0",
  "description": "An Electron application with Svelte and TypeScript",
  "main": "./out/main/index.js",
  "author": "isaxk.com",
  "homepage": "https://ytdesk.isaxk.com",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac --universal",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.31.3",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@xhayper/discord-rpc": "^1.2.0",
    "bits-ui": "^0.21.13",
    "discord-rich-presence": "^0.0.8",
    "discord-rpc": "^4.0.1",
    "electron-json-config": "2.0.0-beta",
    "electron-serve": "^2.1.0",
    "electron-settings": "^4.0.4",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.1.7",
    "lucide-svelte": "^0.428.0",
    "serve": "^14.2.3",
    "svelte-spa-router": "^4.0.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/node": "^20.14.8",
    "autoprefixer": "^10.4.20",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-svelte": "^2.41.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.2",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "vite": "^5.3.1"
  }
}
