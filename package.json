

{
	"name": "ytdesk",
	"productName": "YT Desk",
	"version": "0.2.4",
	"description": "An Electron application with Svelte and TypeScript",
	"main": "./out/main/index.js",
	"author": "example.com",
	"homepage": "https://electron-vite.org",
	"scripts": {
		"format": "prettier --plugin prettier-plugin-svelte --write .",
		"lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
		"typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
		"svelte-check": "svelte-check --tsconfig ./tsconfig.json",
		"typecheck": "npm run typecheck:node && npm run svelte-check",
		"start": "electron-vite preview",
		"dev": "electron-vite dev",
		"build": "npm run build && electron-builder build",
		"postinstall": "electron-builder install-app-deps",
		"build:unpack": "npm run build && electron-builder --dir",
		"build:windows": "npm run build && electron-builder --win --x64",
		"build:macos": "npm run build && electron-builder --mac",
		"build:linux": "npm run build && electron-builder --linux"
	},
	"dependencies": {
		"@cliqz/adblocker-electron": "^1.30.0",
		"@electron-toolkit/preload": "^3.0.1",
		"@electron-toolkit/utils": "^3.0.0",
		"@jaames/iro": "^5.5.2",
		"@melloware/coloris": "^0.24.0",
		"@xhayper/discord-rpc": "^1.1.2",
		"bits-ui": "^0.21.12",
		"discord-rich-presence": "^0.0.8",
		"electron-json-config": "^2.0.0-beta",
		"electron-store": "^10.0.0",
		"hex-to-css-filter": "^5.4.0",
		"lucide-svelte": "^0.414.0",
		"update-electron-app": "^3.0.0"
	},
	"devDependencies": {
		"@electron-toolkit/eslint-config-prettier": "^2.0.0",
		"@electron-toolkit/eslint-config-ts": "^2.0.0",
		"@electron-toolkit/tsconfig": "^1.0.1",
		"@rollup/plugin-multi-entry": "^6.0.1",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/node": "^20.14.8",
		"autoprefixer": "^10.4.19",
		"electron": "^31.0.2",
		"electron-builder": "^24.13.3",
		"electron-vite": "^2.3.0",
		"eslint": "^8.57.0",
		"eslint-plugin-svelte": "^2.41.0",
		"postcss": "^8.4.39",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^4.2.18",
		"svelte-awesome-color-picker": "^3.1.2",
		"svelte-awesome-icons": "^1.2.1",
		"svelte-check": "^3.8.2",
		"svelte-simple-icons": "^1.0.3",
		"tailwindcss": "^3.4.6",
		"tslib": "^2.6.3",
		"typescript": "^5.5.2",
		"vite": "^5.3.1"
	}
}
